<?xml version="1.0" encoding="UTF-8"?>
<beans default-destroy-method="cleanUp" default-init-method="myInit" xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd">

	<!-- Definition of base package mapped through annotation into bean class -->
	<context:component-scan base-package="org.ennva.spring_maven_tutorial"></context:component-scan>

	<bean autowire="byName" class="org.ennva.spring_maven_tutorial.Triangle">
	</bean>

	<bean class="org.ennva.spring_maven_tutorial.Point" id="pointA">
		<property name="x" value="0"></property>
		<property name="y" value="0"></property>
	</bean>

	<bean class="org.ennva.spring_maven_tutorial.Point" id="pointB">
		<property name="x" value="-20"></property>
		<property name="y" value="0"></property>
	</bean>

	<bean class="org.ennva.spring_maven_tutorial.Point" id="pointC">
		<property name="x" value="20"></property>
		<property name="y" value="0"></property>
	</bean>

</beans>
